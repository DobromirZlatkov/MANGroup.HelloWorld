{
    "extends": "airbnb-typescript-prettier",
    "rules": {
      "func-names": ["error", "as-needed"],
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "semi": false,
          "printWidth": 120,
          "endOfLine": "auto"
        }
      ],
      "react/function-component-definition": ["error", {
        "namedComponents": "arrow-function",
        "unnamedComponents": "arrow-function"
      }]
    }
  }